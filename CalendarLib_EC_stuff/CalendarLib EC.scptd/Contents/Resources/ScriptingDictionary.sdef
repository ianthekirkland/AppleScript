    <command name="fetch store" code="!Cls!fst" description="Fetches the event store, prompting authorization if necessary."><result description="The event store. You need this before you can fetch calendars and events."><type type="any"></type></result></command><command name="fetch calendar" code="!CLs!fca" description="Fetches a calendar."><direct-parameter description="The name of the calendar."><type type="text"></type></direct-parameter><parameter description="The type of calendar. One of cal local/cal cloud/cal exchange/cal subscription/cal birthday." name="cal type" code="!Cty" optional="NO"><type type="cal types"></type></parameter><parameter description="The event store, as returned by the 'fetch store' command." name="event store" code="!Cst" optional="NO"><type type="any"></type></parameter><result description="The designated calendar, or missing value if none found."><type type="any"></type></result></command><command name="fetch calendars" code="!CLs!fcA" description="Fetches a list of calendars. Requires version 1.1 of CalendarLib."><direct-parameter description="A list of calendar names. Pass an empty list to include all names."><type type="text" list="yes"></type></direct-parameter><parameter name="cal type list" description="A list of calendar types, from cal local/cal cloud/cal exchange/cal subscription/cal birthday. Pass an empty list for all types. Default is all types." code="!CtY" optional="yes"><type type="cal types" list="yes"></type></parameter><parameter description="The event store, as returned by the 'fetch store' command." name="event store" code="!Cst" optional="NO"><type type="any"></type></parameter><result description="A list of calendars."><type type="any" list="yes"></type></result></command><enumeration name="cal types" code="!Tct" description="Calendar types"><enumerator name="cal local" code="!TtL" description="Local."></enumerator><enumerator name="cal cloud" code="!TtC" description="iCloud/CalDAV."></enumerator><enumerator name="cal exchange" code="!TtE" description="Exchange."></enumerator><enumerator name="cal subscription" code="!TtS" description="Subscription."></enumerator><enumerator name="cal birthday" code="!TtB" description="Birthday."></enumerator></enumeration><command name="fetch events by UID" code="!CLs!feU" description="Fetch events matching the provided UID."><direct-parameter description="The UID of the event."><type type="text"></type></direct-parameter><parameter name="event store" description="The event store, as returned by the 'fetch store' command." code="!Cst" optional="NO"><type type="any"></type></parameter><result description="A list of matching events (there can be more than one match in some cases)."><type type="any" list="yes"></type></result></command><command name="fetch events" code="!CLs!feS" description="Fetch events that occur within a specified time span."><parameter description="The start date. Can be an AppleScript date or Cocoa NSDate." name="starting date" code="!Csd" optional="NO"><type type="any"></type></parameter><parameter name="ending date" description="The end date.  Can be an AppleScript date or Cocoa NSDate." code="!Ced" optional="NO"><type type="any"></type></parameter><parameter description="List or array of calendars to search." name="searching cals" code="!Csc" optional="NO"><type type="any" list="yes"></type></parameter><parameter description="The event store, as returned by the 'fetch store' command." name="event store" code="!Cst" optional="NO"><type type="any"></type></parameter><result description="A list of matching events, sorted by start date."><type type="any" list="yes"></type></result></command><command name="filter events" code="!CLs!fe1" description="Filter a list of events by matching the values with those of the following parameters."><parameter name="event list" description="The list or array of events to filter." code="!Cel" optional="NO"><type type="any" list="yes"></type></parameter><parameter description="The summary/title of the event to match. Omit or pass missing value to ignore it." name="event summary" code="!Csu" optional="yes"><type type="text"></type></parameter><parameter description="The location of the event. Omit or pass missing value to ignore it." name="event location" code="!Clo" optional="yes"><type type="any"></type></parameter><parameter description="The event's description/notes. Omit or pass missing value to ignore it." name="event description" code="!CdD" optional="yes"><type type="text"></type></parameter><parameter description="Whether the event has attendees. Omit or pass missing value to ignore it." name="has attendees" code="!Cha" optional="yes"><type type="boolean"></type></parameter><parameter description="Whether the event is recurring. Omit or pass missing value to ignore it." name="has recurrence" code="!Cre" optional="yes"><type type="boolean"></type></parameter><parameter description="Whether the event is an all-day event. Omit or pass missing value to ignore it." name="runs all day" code="!Cad" optional="yes"><type type="boolean"></type></parameter><result description="Filtered list of events."><type type="any" list="yes"></type></result></command><command name="filter events by pattern" code="!CLs!fe2" description="Filter a list of events by matching the values with the patterns supplied in the following parameters."><parameter name="event list" description="The list or array of events to filter." code="!Cel" optional="NO"><type type="any" list="yes"></type></parameter><parameter description="The pattern for the summary/title of the event to match. Omit or pass missing value to ignore it." name="event summary" code="!Csu" optional="yes"><type type="any"></type></parameter><parameter description="The pattern for the description/notes of the event. Omit or pass missing value to ignore it." name="event description" code="!CdD" optional="yes"><type type="any"></type></parameter><parameter description="The pattern for the location of the event. 